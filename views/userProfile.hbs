{{#if user}}
<div class="windowMainProfile">
            <div class="mainaboutUser">
                              <div class="mainformUser">
                                <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcT02opOAZ9DQHHHlpvxV_1DsjeYGGAbjEvg5w&usqp=CAU"
                                  width="30px" height="30px">
                                <h2>Личный кабинет</h2>
                              </div>
                            <form id="signinForm" method="POST" action="">
                              <div class="form-group">
                                <p>Полное имя: {{user.fullname}}</p>
                              </div>
                              <div class="old">
                                <p>Возраст: {{user.age}} лет</p>
                              </div>
                              <div class="city">
                                <p>Город: {{user.city}}</p>
                              </div>
                              <div class="aboutMe">
                                <p> О себе: {{user.about_me}}</p>
                              </div>
                            </form>
            </div>
  
            {{#if userlogIn.isEditor}}
            <div class="aboutMe">
              <button id="{{user.id}}" class="btn btnEditProfile">Редактировать</button>
            </div>
            {{/if}}
   <div class="mainRooms">


                              {{#each rooms}}
                                <div class="rout"  id="{{this.id}}">
                                  <img src="https://static-cse.canva.com/blob/195615/paul-skorupskas-7KLa-xLbSXA-unsplash-2.jpg"
                                    alt="sfgdsfghfd" class="img" width="150px" height="150px">
                                    <div style="margin-left: 10px;">
                                      <h3>{{this.title}}</h3>
                                      {{!-- <p>Рейтинг: {{this.rating}}</p>
                                      <p>Расстояние: {{this.distance}}</p> --}}
                                      <button class="btn btnInfo" id="{{this.id}}">Войти</button>
                                                    <button class="btn btnDelet" id="555">Удалить</button>

                                    </div>
                                  </div>
                              {{/each}}
     </div>
  </div>
<div class="allRoom">
    
      {{#each rooms}}
       <div class="rout"  id="{{this.id}}">
          <img src="https://static-cse.canva.com/blob/195615/paul-skorupskas-7KLa-xLbSXA-unsplash-2.jpg"
            alt="" class="img" width="150px" height="150px">
            <div style="margin-left: 10px;">
              <h3>{{this.title}}</h3>

              <button class="btn btnInfo" id="555">Войти</button>
              </div>
              </div>
      {{/each}}
  </div>

{{/if}}



